{
    "name": "goodwe-backend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "node --watch src/server.js",
        "start": "node src/server.js",
        "seed": "node src/seed.js",
        "build:frontend": "npm --prefix ../frontend run build",
        "start:all": "npm run build:frontend && node src/server.js",
        "db:migrate": "sequelize-cli db:migrate",
        "db:migrate:undo": "sequelize-cli db:migrate:undo"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "ajv": "^8.17.1",
        "better-sqlite3": "^11.5.0",
        "compression": "^1.8.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "goodwe-app": "file:..",
        "mqtt": "^5.10.0",
        "pg": "^8.12.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.3",
        "sequelize-cli": "^6.6.2"
    }
}
